/* 单行多余文本省略 */
.omit {
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
/* 两行多余文本省略 */
.two_omit {
	overflow: hidden;
	display: -webkit-box;
	word-break: break-all;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
}
